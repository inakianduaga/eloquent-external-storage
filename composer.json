{
    "name": "inakianduaga/eloquent-external-storage",
    "description": "Adds transparent external storage to an eloquent model",
    "keywords": ["eloquent"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Inaki Anduaga",
            "email": "inaki.anduaga@tado.com"
        }
    ],
    "support" : {
        "issues" : "https://github.com/inakianduaga/eloquent-external-storage/issues"
    },
    "require": {
        "php": ">=5.5.0",
        "illuminate/support" : "4.2.x",
        "illuminate/database": "4.2.x",
        "illuminate/console" : "4.2.x",
        "illuminate/config" : "4.2.x",
        "aws/aws-sdk-php" : "dev-master",
        "orchestra/testbench" : "2.2.x"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.3.*@dev",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "InakiAnduaga\\EloquentExternalStorage\\": "src/",
            "InakiAnduaga\\EloquentExternalStorage\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev"
}